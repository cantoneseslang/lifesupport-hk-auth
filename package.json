{
  "name": "google-survey-automation",
  "version": "1.0.0",
  "description": "Google Survey Automation System",
  "main": "Code.gs",
  "scripts": {
    "sync": "git add . && git commit -m 'Auto sync $(date)' && git push origin master && clasp push",
    "push": "clasp push",
    "pull": "clasp pull",
    "deploy": "npm run sync",
    "watch": "nodemon --watch . --ext gs,html,js --exec 'npm run sync'"
  },
  "keywords": [
    "google-apps-script",
    "survey",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/google-apps-script": "^2.0.7",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "nodemon": "^3.0.1"
  }
}